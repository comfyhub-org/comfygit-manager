meta {
  name: List Installed Custom Nodes
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/v2/customnode/installed
  body: none
  auth: none
}

docs {
  Returns installed custom nodes with registry IDs, versions, and enabled status.

  Response format:
  {
    "node_identifier": {
      "cnr_id": "registry_id",
      "aux_id": "user/repo",
      "ver": "1.0.0|latest|nightly|dev",
      "enabled": true|false,
      "name": "Node Display Name"
    }
  }
}
