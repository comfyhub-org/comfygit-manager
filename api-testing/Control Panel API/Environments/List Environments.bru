meta {
  name: List Environments
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/v2/comfygit/environments
  body: none
  auth: none
}

docs {
  List all environments in workspace.

  Response:
  {
    "environments": [
      {
        "name": "default",
        "is_current": true,
        "path": "/path/to/workspace/environments/default",
        "created_at": null,
        "workflow_count": 5,
        "node_count": 12,
        "model_count": 3,
        "current_branch": "main"
      }
    ],
    "current": "default",
    "is_managed": true
  }

  If not in managed workspace: { "environments": [], "current": null, "is_managed": false }
}
