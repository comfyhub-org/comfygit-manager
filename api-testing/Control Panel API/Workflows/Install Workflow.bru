meta {
  name: Install Workflow Dependencies
  type: http
  seq: 4
}

post {
  url: {{baseUrl}}/v2/comfygit/workflow/{{workflowName}}/install
  body: none
  auth: none
}

docs {
  Install all missing node dependencies for a workflow.
  Does NOT install models (models require separate handling).

  Path params:
  - name: Workflow filename

  Response:
  - 200 {
      "status": "success",
      "message": "All dependencies already installed|Installed N node packages",
      "nodes_installed": ["package-id-1", "package-id-2"]
    }
  - 500 { "status": "error", "message": "..." }
}
