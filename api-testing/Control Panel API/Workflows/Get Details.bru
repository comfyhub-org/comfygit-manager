meta {
  name: Get Workflow Details
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/v2/comfygit/workflow/{{workflowName}}/details
  body: none
  auth: none
}

docs {
  Get detailed information about a specific workflow.

  Path params:
  - name: Workflow filename

  Response:
  - 404 { "error": "Workflow not found" }
  - 200 {
      "name": "workflow.json",
      "sync_state": "synced|new|modified|deleted",
      "has_issues": false,
      "issue_summary": "...",
      "uninstalled_count": 0,
      "resolution": {
        "nodes_resolved": [...],
        "nodes_unresolved": [...],
        "models_resolved": [...],
        "models_unresolved": [...],
        "models_ambiguous": [...]
      }
    }
}
