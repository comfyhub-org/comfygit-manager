meta {
  name: Get Workspace Logs
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/v2/workspace/debug/logs?level=INFO&lines=50
  body: none
  auth: none
}

params:query {
  level: INFO
  lines: 50
}

docs {
  Get workspace-level debug logs (cross-environment operations).

  Query params:
  - level: Filter by log level (ERROR|WARNING|INFO|DEBUG, optional)
  - lines: Number of log records to return from end of file (default: 100)

  Response: Same format as environment logs
  [
    {
      "timestamp": "2025-11-23 15:30:45,234",
      "name": "comfygit_core.workspace",
      "level": "INFO",
      "func": "create_environment",
      "line": "123",
      "message": "Created environment: production"
    }
  ]

  Log file location: {workspace}/logs/workspace/full.log
}
