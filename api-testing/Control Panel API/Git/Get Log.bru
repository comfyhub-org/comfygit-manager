meta {
  name: Get Commit Log
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/v2/comfygit/log?limit=20&offset=0
  body: none
  auth: none
}

params:query {
  limit: 20
  offset: 0
}

docs {
  Get commit history with pagination.

  Query params:
  - limit: Number of commits to return (default: 20)
  - offset: Number of commits to skip (default: 0)

  Response:
  {
    "commits": [{ "hash": "...", "message": "...", "author": "...", "date": "..." }],
    "has_more": true|false,
    "current_branch": "main"
  }
}
